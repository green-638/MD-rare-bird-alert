{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(^((?!api).)*)",
            "dest": "/"
        },
        {
            "src": "/^(api.*)",
            "dest": "/api/email"
        }
    ],
    "crons": [
        {
            "path": "/api/email",
            "schedule": "10 23 * * *"
        }
    ]
}

